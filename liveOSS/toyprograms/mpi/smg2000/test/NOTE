#expcreate -f "/opt/openmpi-1.4.2/bin/orterun -np 2 smg2000 -n 30 30 30" pcsamp 
module load openmpi-1.4.2
module load openss-mrnet
setenv OPENSS_DEBUG_MRNET_WATCHER 1
openss -cli -online
expcreate -f "/opt/openmpi-1.4.2/bin/orterun -np 2 smg2000 -n 40 40 40" mpit 
