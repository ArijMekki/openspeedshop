openss -offline -f "/opt/openmpi-1.2.6/bin/orterun -np 2 smg2000 -n 65 65 65" usertime 
openss -offline -f "/opt/openmpi-1.2.6/bin/orterun -np 2 smg2000 -n 65 65 65" mpit

