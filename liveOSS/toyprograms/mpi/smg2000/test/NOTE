# offline case
module load openmpi-1.4.2
module load openss-mrnet

openss -offline -f "/opt/openmpi-1.4.2/bin/orterun -np 2 smg2000 -n 65 65 65" pcsamp 
or
osspcsamp "/opt/openmpi-1.4.2/bin/orterun -np 2 smg2000 -n 65 65 65"

openss -offline -f "/opt/openmpi-1.4.2/bin/orterun -np 2 smg2000 -n 65 65 65" usertime 
or
ossusertime "/opt/openmpi-1.4.2/bin/orterun -np 2 smg2000 -n 65 65 65"

openss -offline -f "/opt/openmpi-1.4.2/bin/orterun -np 2 smg2000 -n 65 65 65" mpit
or
ossmpit "/opt/openmpi-1.4.2/bin/orterun -np 2 smg2000 -n 65 65 65"

